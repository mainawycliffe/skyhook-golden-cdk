{
  "name": "@skyhookadventure/golden-cdk",
  "license": "MIT",
  "version": "2.0.4",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --resolveJsonModule",
    "watch": "tsc -w --resolveJsonModule",
    "test": "./node_modules/.bin/jest",
    "coverage": "./node_modules/.bin/jest --collect-coverage",
    "lint": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "bump": "./node_modules/.bin/versiony package.json --patch"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.68.0",
    "@aws-cdk/aws-appsync-alpha": "^2.59.0-alpha.0",
    "@types/jest": "^29.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "aws-cdk-lib": "^2.113.0",
    "cdk-lambda-nodejs-mock": "^2.0.2",
    "constructs": "^10.3.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.3.2",
    "versiony-cli": "^1.3.0"
  },
  "jest": {
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleNameMapper": {
      "aws-cdk-lib/aws-lambda-nodejs": "cdk-lambda-nodejs-mock"
    }
  },
  "peerDependencies": {
    "@aws-cdk/assert": "^2",
    "@aws-cdk/aws-appsync-alpha": "^2.25.0-alpha.0",
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  },
  "packageManager": "yarn@4.0.2"
}
